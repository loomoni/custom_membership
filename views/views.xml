<odoo>
    <!--    tree tree  -->
    <record id="register_form_tree_view" model="ir.ui.view">
        <field name="name">register.form.view</field>
        <field name="model">form.registration</field>
        <field name="arch" type="xml">
            <tree string="Register Members">
                <field name="name"/>
                <field name="membership_number"/>
                <field name="certificate"/>
                <field name="year_establishment"/>
                <field name="business_no"/>
                <field name="membership_cat"/>
                <field name="company_status"/>
                <field name="chairperson_name"/>
                <field name="executive_name"/>
            </tree>
        </field>
    </record>

    <!--    form view -->
    <record id="register_form_form_view" model="ir.ui.view">
        <field name="name">register.form.form.view</field>
        <field name="model">form.registration</field>
        <field name="arch" type="xml">
            <form string="Register Members">
                <sheet>
                    <div class="oe_title">
                        <h1>
                            Registration Form
                        </h1>
                    </div>
                    <group string="Member Details">
                        <group>
                            <field name="name"/>
                            <field name="membership_number"/>
                            <field name="certificate"/>
                            <field name="year_establishment"/>
                            <field name="business_no"/>
                        </group>
                        <group>
                            <field name="company_status"/>
                            <field name="chairperson_name"/>
                            <field name="executive_name"/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="date_registration"/>
                            <field name="copy_registration_certificate_attachment"
                                   filename="copy_registration_certificate_file_name"/>
                            <field name="copy_registration_certificate_file_name" invisible="1"/>
                        </group>
                        <group>
                            <field name="sector_industry"/>
                            <field name="cluster_id"/>
                            <field name="membership_cat"/>
                            <field name="applicable_fee"/>
                            <field name="annual_fee"/>
                        </group>
                        <group string="Address">
                            <field name="street" placeholder="street"/>
                            <field name="street_two" placeholder="street 2"/>
                            <field name="region"/>
                            <field name="arusha_districts" attrs="{'invisible':[('region', '!=', 'arusha')]}"/>
                            <field name="dar_districts" attrs="{'invisible':[('region', '!=', 'dar')]}"/>
                            <field name="dodoma_districts" attrs="{'invisible':[('region', '!=', 'dodoma')]}"/>
                            <field name="geita_districts" attrs="{'invisible':[('region', '!=', 'geita')]}"/>
                            <field name="iringa_districts" attrs="{'invisible':[('region', '!=', 'iringa')]}"/>
                            <field name="kagera_districts" attrs="{'invisible':[('region', '!=', 'kagera')]}"/>
                            <field name="katavi_districts" attrs="{'invisible':[('region', '!=', 'katavi')]}"/>
                            <field name="kigoma_districts" attrs="{'invisible':[('region', '!=', 'kigoma')]}"/>
                            <field name="kilimanjaro_districts"
                                   attrs="{'invisible':[('region', '!=', 'kilimanjaro')]}"/>
                            <field name="lindi_districts" attrs="{'invisible':[('region', '!=', 'lindi')]}"/>
                            <field name="manyara_districts" attrs="{'invisible':[('region', '!=', 'manyara')]}"/>
                            <field name="mara_districts" attrs="{'invisible':[('region', '!=', 'mara')]}"/>
                            <field name="mbeya_districts" attrs="{'invisible':[('region', '!=', 'mbeya')]}"/>
                            <field name="morogoro_districts" attrs="{'invisible':[('region', '!=', 'morogoro')]}"/>
                            <field name="mtwara_districts" attrs="{'invisible':[('region', '!=', 'mtwara')]}"/>
                            <field name="mwanza_districts" attrs="{'invisible':[('region', '!=', 'mwanza')]}"/>
                            <field name="njombe_districts" attrs="{'invisible':[('region', '!=', 'njombe')]}"/>
                            <field name="pwani_districts" attrs="{'invisible':[('region', '!=', 'pwani')]}"/>
