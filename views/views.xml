                        <!--                                    <field name="phone"/>-->
                        <!--                                    <field name="email"/>-->
                        <!--                                    <field name="mobile"/>-->
                        <!--                                    <field name="general_contact_id" invisible="1"/>-->
                        <!--                                </tree>-->
                        <!--                            </field>-->
                        <!--                        </page>-->

                        <page string="General Contact">
                            <field name="general_contact_lines_ids">
                                <kanban>
                                    <field name="name"/>
                                    <field name="title"/>
                                    <field name="job_position"/>
                                    <field name="phone"/>
                                    <field name="email"/>
                                    <field name="mobile"/>
                                    <field name="general_contact_id" invisible="1"/>
                                    <templates>
                                        <t t-name="kanban-box">
                                            <div class="oe_kanban_global_click">
                                                <div class="o_kanban_image">
