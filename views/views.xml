                                                <div class="oe_kanban_details">
                                                    <strong class="o_kanban_record_title">
                                                        <field name="name"/>
                                                    </strong>
                                                    <div name="tags"/>
                                                    <ul>
                                                        <li>Title:
                                                            <field name="title"/>
                                                        </li>
                                                    </ul>
                                                    <div name="tags"/>
                                                    <ul>
                                                        <li>Position:
                                                            <field name="job_position"/>
                                                        </li>

                                                    </ul>
                                                    <div name="tags"/>
                                                    <ul>
                                                        <li>Email:
                                                            <field name="email"/>
                                                        </li>
                                                    </ul>
                                                    <div name="tags"/>
                                                    <ul>
                                                        <li>Phone:
                                                            <field name="phone"/>
                                                        </li>
                                                    </ul>
                                                    <!--                                                    <div name="tags"/>-->
                                                    <!--                                                    <ul>-->
                                                    <!--                                                        <li>-->
                                                    <!--                                                              <field name="mobile"/>-->
                                                    <!--                                                        </li>-->
