    <!--    Payment part-->
    <!--    Tree View -->
    <record id="payment_tree_view" model="ir.ui.view">
        <field name="name">Payment</field>
        <field name="model">payment</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name"/>
                <field name="category"/>
                <field name="cluster"/>
                <field name="type"/>
                <field name="pay_year"/>
                <field name="fee_amount" sum="Fee Amount"/>
                <field name="amount_paid" sum="Amount Paid"/>
                <field name="amount_remain" sum="Amount Remain"/>
<!--                <field name="is_active"/>-->
                <field name="state"/>
            </tree>
        </field>
    </record>

    <!--    Form View -->
    <record id="payment_form_view" model="ir.ui.view">
        <field name="name">Payment</field>
        <field name="model">payment</field>
        <field name="arch" type="xml">
            <form string="">
                <header>
                    <button name="button_approve" string="paid" class="oe_highlight" states="unpaid,partial"
                            type="object"/>
                    <button name="button_partial_paid" string="partial paid" states="unpaid,partial" type="object"/>
                    <field name="state" widget="statusbar" statusbar_visible="unpaid,partial,paid"
                           statusbar_colors="{'KEY_IS_STATE':'VALUE_IS_COLOR'}"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="category"/>
                            <field name="cluster"/>
                            <field name="fee_amount"/>
                            <field name="pay_year"/>

                        </group>
                        <group>
<!--                            <field name="pay_date"/>-->
                            <field name="type"/>
                            <field name="amount_paid" />
<!--                            attrs="{'readonly':[('state','in',['paid'])]}"-->
                            <field name="amount_remain"/>
                        </group>
                    </group>
                    <notebook>
                         <page string="Add Payments">
                            <field name="payment_lines_ids">
                                <tree editable="bottom">
                                    <field name="payment_date"/>
                                    <field name="amount_payment"/>
                                    <field name="receipt" filename="receipt_file_name"/>
                                    <field name="receipt_file_name" invisible="1"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

